<h1>Example Autocomplete</h1>

<p>{{data}}</p>

<form action="#endpoint" method="post" id="searchForm" class="c-autocomplete-form">
	<div>
		<label for="searchFormTextInput" class="c-autocomplete-form__label">Search for an Animal:</label>
		<input type="text" autocomplete="off" data-component-autocomplete id="searchFormTextInput" class="c-autocomplete">
	</div>
	<button type="submit">Search</button>
</form>

