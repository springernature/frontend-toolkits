<div class="c-corporate-footer">
    <div class="c-corporate-footer__container">
		{{#with navigation}}
			<nav>
				<ul class="c-corporate-footer__links">
					{{#each links}}
					<li>
						{{#if this.url}}
							<a class="c-corporate-footer__link" href="{{this.url}}">{{text}}</a>
						{{/if}}
						{{#if buttonProperties}}
							<button class="c-corporate-footer__link" {{{buttonProperties}}}>{{text}}</button>
						{{/if}}
					</li>
					{{/each}}
				</ul>
			</nav>
		{{/with}}
		{{#with image}}
			<a href="{{link}}" class="c-corporate-footer__link">
				<img src="{{src}}" alt="{{alt}}" loading="lazy" width="200" height="20"/>
			</a>
		{{/with}}
		<p class="c-corporate-footer__legal" data-test="copyright">&copy; {{currentYear}} Springer Nature</p>
    </div>
</div>
